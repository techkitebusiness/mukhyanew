    <script>
        $(document).ready(function () {
            $('#submit').click(function (e) {
                e.preventDefault(); // Prevent default form submission

                // Get form inputs
                let username = $('#username').val().trim();
                let password = $('#passwordInput').val().trim();
                let invite = $('#invite').val().trim();
                let checkbox = $('.van-checkbox__icon').hasClass('van-checkbox__icon--checked');

                // Validate form inputs
                if (!username || !password || !invite || !checkbox) {
                    alert('Please fill in all required fields.');
                    return;
                }

                // Make AJAX request
                $.ajax({
                    type: "POST",
                    url: "/api/webapi/register",
                    data: {
                        username: username,
                        pwd: password,
                        invitecode: invite
                    },
                    dataType: "json",
                    success: function (response) {
                        // Handle response from server
                        alert(response.message);
                        if (response.status === true) {
                            // Redirect to login page on successful registration
                            window.location.href = '/login';
                        }
                    },
                    error: function (xhr, status, error) {
                        // Handle AJAX errors
                        console.error(xhr.responseText);
                        alert('An error occurred while processing your request. Please try again later.');
                    }
                });
            });

            // Function to toggle password visibility
           
        });
    </script>
    <script>
        // Get all eye icons and password input fields
const eyeIcons = document.querySelectorAll('.eye');
const passwordInputs = document.querySelectorAll('input[type="password"]');

// Add event listener to each eye icon
eyeIcons.forEach((eyeIcon, index) => {
    eyeIcon.addEventListener('click', () => {
        const passwordInput = passwordInputs[index];
        // Toggle the type attribute of the corresponding password input
        if (passwordInput.type === 'password') {
            passwordInput.type = 'text';
            eyeIcon.src = '/images/eyeInvisible-821d9d16.png'; // Change the eye icon to visible
        } else {
            passwordInput.type = 'password';
            eyeIcon.src = '/images/eyeInvisible-821d9d16.png'; // Change the eye icon to invisible
        }
    });
});

    </script>
