<script>
	// JavaScript code to show the popup on page load and dim the background
	window.onload = function() {
		var popupContainer = document.getElementById('popupContainer');
		var dimmedOverlay = document.getElementById('dimmedOverlay');
		var closeButton = popupContainer.querySelector('.van-dialog__confirm'); // Assuming this is the close button
		popupContainer.style.display = 'block'; // Show the popup
		dimmedOverlay.style.display = 'block'; // Show the dimmed overlay
	
		// Close the popup when the close button is clicked
		closeButton.addEventListener('click', function() {
			popupContainer.style.display = 'none'; // Hide the popup
			dimmedOverlay.style.display = 'none'; // Hide the dimmed overlay
		});
	};
	
    // Get the draggable element
    var draggableElement = document.getElementById('customerId');
    
    // Variables to store mouse position
    var offsetX = 0;
    var offsetY = 0;

    // Function to handle touch start event
    function handleTouchStart(e) {
        offsetX = e.touches[0].clientX - draggableElement.getBoundingClientRect().left;
        offsetY = e.touches[0].clientY - draggableElement.getBoundingClientRect().top;
    }

    // Function to handle touch move event
    function handleTouchMove(e) {
        e.preventDefault();
        var x = e.touches[0].clientX - offsetX;
        var y = e.touches[0].clientY - offsetY;
        
        // Update the position of the draggable element
        draggableElement.style.left = x + 'px';
        draggableElement.style.top = y + 'px';
    }

    // Add event listeners for touch events
    draggableElement.addEventListener('touchstart', handleTouchStart, false);
    draggableElement.addEventListener('touchmove', handleTouchMove, false);
</script>

